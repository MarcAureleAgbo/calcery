---

import '../styles/global.css';

import SEO from '../components/SEO.astro';

import Brand from '../components/ui/Brand.astro';

export interface Props {

  title?: string;

  description?: string;

  canonical?: string;

}

const { title = "Calcery", description = "Calculateurs simples du quotidien", canonical } = Astro.props;

